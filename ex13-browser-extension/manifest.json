{
    "name": "Word seeker",
    "version": "1.0",
    "description": "Seeks for words using google search",
    "permissions": ["storage"],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [{
      "js" : ["popup.js"]
    }],
    "manifest_version": 2
}
